{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/api/**/*.ts",
              "apps/api/**/*.spec.ts",
              "apps/api/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/api"
          ]
        }
      }
    },
    "fetch": {
      "root": "libs/fetch",
      "sourceRoot": "libs/fetch/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/fetch/**/*.ts",
              "libs/fetch/**/*.spec.ts",
              "libs/fetch/**/*.spec.tsx",
              "libs/fetch/**/*.spec.js",
              "libs/fetch/**/*.spec.jsx",
              "libs/fetch/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/fetch/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/fetch"
          ]
        }
      }
    },
    "helper": {
      "root": "libs/helper",
      "sourceRoot": "libs/helper/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/helper/**/*.ts",
              "libs/helper/**/*.spec.ts",
              "libs/helper/**/*.spec.tsx",
              "libs/helper/**/*.spec.js",
              "libs/helper/**/*.spec.jsx",
              "libs/helper/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/helper/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/helper"
          ]
        }
      }
    },
    "web-next": {
      "root": "apps/web-next",
      "sourceRoot": "apps/web-next",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/web-next",
            "outputPath": "dist/apps/web-next"
          },
          "configurations": {
            "production": {}
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "web-next:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "web-next:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "web-next:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/web-next/**/*.{ts,tsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/web-next/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/web-next"
          ]
        }
      }
    },
    "icons": {
      "root": "libs/icons",
      "sourceRoot": "libs/icons/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/icons/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/icons/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/icons"
          ]
        }
      }
    },
    "datasource": {
      "root": "libs/datasource",
      "sourceRoot": "libs/datasource/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/datasource/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/datasource/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/datasource"
          ]
        }
      }
    },
    "state-engine": {
      "root": "libs/state-engine",
      "sourceRoot": "libs/state-engine/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/state-engine/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/state-engine/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/state-engine"
          ]
        }
      }
    },
    "constants": {
      "root": "libs/constants",
      "sourceRoot": "libs/constants/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/constants/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/constants/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/constants"
          ]
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint",
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "web-next"
}