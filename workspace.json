{
  "version": 1,
  "projects": {
    "poster": {
      "root": "apps/poster",
      "sourceRoot": "apps/poster",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/poster",
            "outputPath": "dist/apps/poster"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/poster/environments/environment.ts",
                  "with": "apps/poster/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/next:dev-server",
          "options": {
            "buildTarget": "poster:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "poster:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "poster:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/poster/.eslintrc",
            "tsConfig": [
              "apps/poster/tsconfig.json",
              "apps/poster/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/poster/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/poster/jest.config.js",
            "tsConfig": "apps/poster/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "poster-e2e": {
      "root": "apps/poster-e2e",
      "sourceRoot": "apps/poster-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/poster-e2e/cypress.json",
            "tsConfig": "apps/poster-e2e/tsconfig.e2e.json",
            "devServerTarget": "poster:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "poster:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/poster-e2e/.eslintrc",
            "tsConfig": ["apps/poster-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/poster-e2e/**"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/api/.eslintrc",
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "fetch": {
      "root": "libs/fetch",
      "sourceRoot": "libs/fetch/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/fetch/.eslintrc",
            "tsConfig": [
              "libs/fetch/tsconfig.lib.json",
              "libs/fetch/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/fetch/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/fetch/jest.config.js",
            "tsConfig": "libs/fetch/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint",
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "poster"
}
